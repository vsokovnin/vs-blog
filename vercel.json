{
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "src": "/blog/(.*)", "dest": "/blog/$1" },
    { "src": "/blog", "dest": "/blog" },
    { "src": "/(.*)", "dest": "https://sokovnin.tilda.ws/$1", "status": 200 }
  ],
  "headers": [
    {
      "source": "/_astro/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
